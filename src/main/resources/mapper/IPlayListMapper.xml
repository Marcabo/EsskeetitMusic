<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.herion.esskeetitmusic.dao.IPlayListDao">
    <select id="findAll" resultType="com.herion.esskeetitmusic.entity.PlayList">
        select * from PlayList;
    </select>
    <update id="starIncrease" >
        update PlayList set star = star + 1 where listName = ''
    </update>
</mapper>